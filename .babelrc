{
  "presets": [
    [
      "env", {
        "targets": {
          "browsers": ["last 3 versions", "safari >= 10"],
          "loose": true
        }
      }],
      "react",
      "react-optimize"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "react-hot-loader/babel"
  ]
}
